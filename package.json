{
  "name": "@diplodoc/mermaid-extension",
  "version": "1.1.3",
  "description": "Mermaid plugin for Diplodoc transformer and builder",
  "main": "plugin/index.js",
  "types": "plugin/index.d.ts",
  "engines": {
    "node": ">=18",
    "npm": ">=9.*"
  },
  "exports": {
    ".": "./plugin/index.js",
    "./plugin": "./plugin/index.js",
    "./runtime": "./runtime/index.js",
    "./react": "./react/index.js",
    "./hooks": "./react/index.js"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "./esbuild/build.js",
    "build:declarations": "tsc --emitDeclarationOnly --outDir .",
    "prepublish": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest"
  },
  "keywords": [
    "mermaid",
    "markdown-it"
  ],
  "author": "",
  "license": "ISC",
  "files": [
    "plugin",
    "runtime",
    "react"
  ],
  "peerDependencies": {
    "react": "^16.8.0  || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@diplodoc/eslint-config": "^1.0.14",
    "@diplodoc/prettier-config": "^1.0.0",
    "@diplodoc/tsconfig": "^1.0.2",
    "@types/d3": "^7.4.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^20.3.2",
    "@types/react": "^18.0.35",
    "esbuild": "^0.17.12",
    "esbuild-inline-sass": "^0.4.1",
    "eslint": "^8.51.0",
    "jest": "^29.5.0",
    "markdown-it": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@diplodoc/transform": "^4.2.1",
    "@gravity-ui/icons": "^2.3.0",
    "mermaid": "^10.0.2",
    "ts-dedent": "^2.2.0"
  },
  "overrides": {
    "react-sortable-hoc": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}

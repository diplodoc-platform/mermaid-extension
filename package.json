{
  "name": "@diplodoc/mermaid-extension",
  "version": "1.2.3",
  "description": "Mermaid plugin for Diplodoc transformer and builder",
  "main": "build/plugin/index.js",
  "types": "build/plugin/index.d.ts",
  "engines": {
    "node": ">=18",
    "npm": ">=9.*"
  },
  "exports": {
    ".": "./build/plugin/index.js",
    "./plugin": "./build/plugin/index.js",
    "./runtime": "./build/runtime/index.js",
    "./react": "./build/react/index.js",
    "./hooks": "./build/react/index.js"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "./esbuild/build.js",
    "build:declarations": "tsc --emitDeclarationOnly --outDir ./build",
    "prepublishOnly": "npm run build",
    "test": "exit 0",
    "lint": "lint update && lint",
    "lint:fix": "lint update && lint fix",
    "pre-commit": "lint update && lint-staged",
    "prepare": "husky"
  },
  "keywords": [
    "mermaid",
    "markdown-it"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "build"
  ],
  "peerDependencies": {
    "markdown-it": "^13.0.0",
    "react": "^16.8.0  || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@diplodoc/lint": "^1.1.3",
    "@diplodoc/transform": "^4.7.2",
    "@diplodoc/tsconfig": "^1.0.2",
    "@gravity-ui/icons": "^2.8.1",
    "@types/d3": "^7.4.0",
    "@types/markdown-it": "^13.0.9",
    "@types/node": "^20.3.2",
    "@types/react": "^18.0.35",
    "esbuild": "^0.19.9",
    "esbuild-inline-sass": "^0.4.1",
    "jest": "^29.5.0",
    "markdown-it": "^13.0.2",
    "mermaid": "^10.6.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "ts-dedent": "^2.2.0",
    "typescript": "^5.0.2"
  }
}

{
  "name": "@diplodoc/mermaid-extension",
  "version": "1.2.0",
  "description": "Mermaid plugin for Diplodoc transformer and builder",
  "main": "plugin/index.js",
  "types": "plugin/index.d.ts",
  "engines": {
    "node": ">=18",
    "npm": ">=9.*"
  },
  "exports": {
    ".": "./plugin/index.js",
    "./plugin": "./plugin/index.js",
    "./runtime": "./runtime/index.js",
    "./react": "./react/index.js",
    "./hooks": "./react/index.js"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "./esbuild/build.js",
    "build:declarations": "tsc --emitDeclarationOnly --outDir .",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "exit 0"
  },
  "keywords": [
    "mermaid",
    "markdown-it"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "plugin",
    "runtime",
    "react"
  ],
  "peerDependencies": {
    "markdown-it": "^14.0.0",
    "react": "^16.8.0  || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@diplodoc/eslint-config": "^1.0.14",
    "@diplodoc/prettier-config": "^1.0.0",
    "@diplodoc/transform": "^4.7.2",
    "@diplodoc/tsconfig": "^1.0.2",
    "@gravity-ui/icons": "^2.8.1",
    "@types/d3": "^7.4.0",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.3.2",
    "@types/react": "^18.0.35",
    "esbuild": "^0.19.9",
    "esbuild-inline-sass": "^0.4.1",
    "eslint": "^8.51.0",
    "jest": "^29.5.0",
    "markdown-it": "^14.0.0",
    "mermaid": "^10.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "ts-dedent": "^2.2.0",
    "typescript": "^5.0.2"
  }
}
